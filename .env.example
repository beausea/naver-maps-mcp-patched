# 네이버 API 인증 정보
NAVER_CLIENT_ID=your_client_id_here
NAVER_CLIENT_SECRET=your_client_secret_here

# 서버 설정
PORT=3000
LOG_LEVEL=info  # error, warn, info, debug
DEBUG=false      # 디버그 모드 true/false
USE_DUMMY_DATA_WHEN_ERROR=true  # 오류 발생 시 더미 데이터 반환 true/false

# API 설정
API_TIMEOUT=5000  # API 요청 타임아웃 (밀리초)
API_RETRIES=1     # API 재시도 횟수